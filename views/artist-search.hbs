<div class="artist-search">
    {{#each dataArtists }}  {{!-- 'dataArtists' is from line 40 at app.js --}}
    <div class="artist"> 
        <div class="image">
            <img src="{{ images.0.url }}" />  {{!-- image URL taken from here: http://jsfiddle.net/JMPerez/0u0v7e1b/ --}}
        </div>
        <div class="text">
            <h1>{{ name }}</h1>
            <a href ="/albums/{{ id }}" target="_blank">View albums</a>
        </div>
    </div>
    {{/each}}
</div>